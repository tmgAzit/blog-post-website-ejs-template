<%- include("partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="POST">
  <div class="form-group">
    <label for="contentTitle">Title</label>
    <input
      type="text"
      class="form-control"
      id="contentTitle"
      name="postTitle"
      autocomplete="off"
    />
  </div>
  <div class="form-group">
    <label for="postTextarea">Post</label>
    <textarea
      name="postBody"
      class="form-control"
      cols="30"
      rows="5"
      id="postTextarea"
      autocomplete="off"
    ></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer"); -%>
